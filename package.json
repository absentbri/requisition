{
  "name": "requisition",
  "version": "0.0.1",
  "description": "Requests front-end for Sonarr and/or Radarr.",
  "author": "Brian Ramsey",
  "private": true,
  "scripts": {
    "serve:dev": "npm run dev",
    "dev": "nodemon --watch app --watch start --watch providers --watch bootstrap --watch config --watch .env -x node server.js",
    "build": "cross-env NODE_ENV=production node ./ace nuxtbuild",
    "start": "cross-env NODE_ENV=production node server.js",
    "test": "jest"
  },
  "dependencies": {
    "@adonisjs/ace": "^5.0.8",
    "@adonisjs/auth": "^3.1.0",
    "@adonisjs/bodyparser": "^2.0.9",
    "@adonisjs/cors": "^1.0.2",
    "@adonisjs/fold": "^4.0.5",
    "@adonisjs/framework": "^5.0.13",
    "@adonisjs/ignitor": "^2.0.8",
    "@adonisjs/lucid": "^6.1.3",
    "@adonisjs/session": "^1.0.19",
    "@adonisjs/shield": "^1.0.4",
    "@adonisjs/validator": "^5.0.6",
    "@nativecode/radarr": "^4.2.0",
    "@nativecode/sonarr": "^5.2.0",
    "@nuxtjs/auth": "^4.9.0",
    "@nuxtjs/axios": "^5.9.6",
    "@nuxtjs/dotenv": "^1.4.0",
    "@nuxtjs/pwa": "^3.0.0-0",
    "cross-env": "^7.0.2",
    "husky": "^4.2.3",
    "lodash": "^4.17.15",
    "nuxt": "^2.0.0",
    "sqlite3": "^4.1.1"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^0.6.1",
    "@nuxtjs/vuetify": "^1.11.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.2.3",
    "eslint-plugin-vue": "^6.2.2",
    "jest": "^25.2.3",
    "nodemon": "^2.0.2",
    "standardx": "^5.0.0",
    "ts-jest": "^25.0.0",
    "vue-jest": "^3.0.5"
  },
  "standardx": {
    "globals": [
      "use"
    ]
  },
  "eslintConfig": {
    "extends": [
      "plugin:vue/recommended"
    ],
    "rules": {
      "curly": [
        "error",
        "all"
      ]
    },
    "parser": "vue-eslint-parser",
    "parserOptions": {
      "parser": "babel-eslint",
      "ecmaVersion": 2015,
      "sourceType": "module"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "ie >= 11"
  ]
}
